{
    "name": "acey",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dist": "npm run build && npm run prune",
        "start": "node build/prod",
        "start-dev": "node build/dev",
        "build": "webpack --env.buildMode=production --progress --color --display=errors-only",
        "build-debug": "webpack --env.buildMode=debug --progress --color ",
        "watch-debug": "webpack --env.buildMode=debug --progress --color -w",
        "build-release": "webpack --env.buildMode=release --progress --color",
        "watch-release": "webpack --env.buildMode=release --progress --color -w",
        "prune": "npm prune --production"
    },
    "devDependencies": {
        "@babel/core": "7.2.2",
        "@babel/plugin-transform-runtime": "7.2.0",
        "@babel/preset-env": "7.2.3",
        "@babel/runtime": "7.2.0",
        "@types/clone": "0.1.30",
        "@types/hapi": "17.8.2",
        "@types/js-yaml": "3.11.4",
        "@types/lodash": "4.14.119",
        "@types/mkdirp": "0.5.2",
        "@types/nanoid": "1.2.0",
        "@types/node-cleanup": "2.1.0",
        "@types/node-fetch": "2.1.4",
        "@types/qs": "6.5.1",
        "@types/winston": "2.4.4",
        "babel-loader": "8.0.4",
        "clean-webpack-plugin": "1.0.0",
        "ts-loader": "5.3.2",
        "typescript": "3.2.2",
        "webpack": "4.28.2",
        "webpack-cli": "3.1.2",
        "webpack-node-externals": "1.7.2"
    },
    "dependencies": {
        "clone": "2.1.2",
        "hapi": "17.8.1",
        "js-yaml": "3.12.0",
        "lodash": "4.17.11",
        "mkdirp": "0.5.1",
        "nanoid": "2.0.0",
        "node-cleanup": "2.1.2",
        "node-fetch": "2.3.0",
        "qs": "6.6.0",
        "winston": "3.1.0"
    }
}
